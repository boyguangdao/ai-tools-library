# Supabase配置
# 请将以下值替换为您的Supabase项目信息

# 项目URL (在Supabase Dashboard -> Settings -> API -> Project URL)
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# 匿名密钥 (在Supabase Dashboard -> Settings -> API -> anon public)
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# 服务角色密钥 (在Supabase Dashboard -> Settings -> API -> service_role)
# 注意：这个密钥只在服务器端使用，不要暴露给客户端
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# 应用配置
NEXT_PUBLIC_APP_URL=http://localhost:3000 